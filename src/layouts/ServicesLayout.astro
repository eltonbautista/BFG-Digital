---
import Layout from "../layouts/Layout.astro";
import WebsiteLayout from "../components/WebsiteLayout";
import Home from "../components/Home/Home";
import React from "react";
import Navbar from "../components/Navbar";
import ServicesHero from "../components/Services/ServicesHero.astro";
import ServicesAbout from "../components/Services/ServicesAbout.astro";
import ServicesSteps from "../components/Services/ServicesSteps.astro";
import ServicesPricing from "../components/Services/ServicesPricing.astro";
import ServicesFAQ from "../components/Services/ServicesFAQ.astro";
const { content } = Astro.props;
---

<Layout
  title="BFG Interactive: Calgary Website Design and Development | Calgary Website Development | Calgary Search Engine Optimization (SEO) "
>
  <WebsiteLayout client:load>
    <!-- <Home client:idle /> -->
    <main class="bg-secondary-content">
      <ServicesHero content={content} />
      <ServicesAbout content={content} />
      <ServicesSteps content={content} />
      <ServicesPricing content={content} />
      <ServicesFAQ content={content} />
    </main>
  </WebsiteLayout>
</Layout>
